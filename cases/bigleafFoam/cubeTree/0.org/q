FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      q;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "../constant/flowProperties"

dimensions      [0 0 0 0 0 0 0];

internalField   uniform $qRef;

boundaryField
{
    inlet
    {
	    type            fixedValue;
        value           uniform $qRef;
    }

    outlet
    {
	    type            zeroGradient;
    }

    sky
    {
        type            fixedValue;
        value           uniform $qRef;
    }


    ground
    {
        type            fixedValue;
        value           uniform $qRef;
    }

    frontAndBack
    {
        type            zeroGradient;
    }
}


