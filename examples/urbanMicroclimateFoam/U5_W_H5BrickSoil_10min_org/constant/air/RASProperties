FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      RASProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

RASModel        porousrealizableKE;

kEpsilonCoeffs
{
    betaP       1;
    betaD       5.1;
    C4          0.9;
    C5          0.9;
    Cmu              0.09;
    C1               1.44;
    C2               1.92;
    sigmaEps	     1.3;
}

turbulence      on;

printCoeffs     on;


