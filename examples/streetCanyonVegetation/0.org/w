FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      w;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// #include        "../constant/flowProperties"

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0160409;

boundaryField
{

    ground
    {
          type            zeroGradient;
    }

    cube1
    {
          type            zeroGradient;

    }
    cube2
    {
          type            zeroGradient;

    }

    outlet
    {
          type          zeroGradient;
    }


    sky
    {
          type         zeroGradient;
    }

    inlet
    {
          type            fixedValue;
          value           uniform 0.0160409;
    }


    front
    {
          type        symmetryPlane;
    }

    back
    {
          type        symmetryPlane;
    }

}


