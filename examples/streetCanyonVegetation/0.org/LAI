{
      ground
      {
            type            zeroGradient;
      }

      cube1
      {
            type            zeroGradient;

      }
      cube2
      {
            type            zeroGradient;

      }


      outlet
      {
            type          zeroGradient;
      }


      sky
      {
            type         zeroGradient;
      }

      inlet
      {
            type            zeroGradient;
      }


      front
      {
            type        symmetryPlane;
      }

      back
      {
            type        symmetryPlane;
      }
}

