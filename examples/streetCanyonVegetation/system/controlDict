FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs            (
				             "libgroovyBC.so"
                     "libporousIncompressibleRASModels.so"
                );

application     buoyantBoussinesqVegetationSimpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         100;//300000;

deltaT          1;

writeControl    timeStep;

writeInterval   10;//5000;

purgeWrite      1;

writeFormat     ascii;

writePrecision  12;

writeCompression on;

timeFormat      general;

timePrecision   10;

runTimeModifiable true;


