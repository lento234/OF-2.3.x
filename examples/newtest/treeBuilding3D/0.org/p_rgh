FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "../constant/flowProperties"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $pressure;

boundaryField
{

    ground
    {
          type          zeroGradient;
    }

    cube
    {
          type            zeroGradient;
    }

    outlet
    {
          type         fixedValue;
          value        $internalField;
    }

    sky
    {
          type         zeroGradient;
    }

    inlet
    {
          type         zeroGradient;
    }

    front
    {
          type        symmetryPlane;
    }

    back
    {
          type        symmetryPlane;
    }
}

