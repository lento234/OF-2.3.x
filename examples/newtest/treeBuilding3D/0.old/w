FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      w;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// #include        "../constant/flowProperties"

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0160409;

boundaryField
{
    inlet
    {
	       type            fixedValue;
         value           uniform 0.0160409;
    }
    outlet
    {
	       type            zeroGradient;
    }

    sky
    {
          type            zeroGradient;
    }

    ground
    {
          type            zeroGradient;
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }
}


