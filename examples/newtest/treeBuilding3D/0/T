          //type            fixedValue;
          //value           uniform 310;

    }

    outlet
    {
          type          zeroGradient;
    }


    sky
    {
          type         zeroGradient;
    }

    inlet
    {
  	      type            fixedValue;
          value           uniform 303.15;
    }


    front
    {
          type        symmetryPlane;
    }

    back
    {
          type        symmetryPlane;
    }
